<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true">

            <RelativeLayout
                android:id="@+id/one"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp">

                <ImageView
                    android:id="@+id/nineView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine1"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/two"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/one">

                <ImageView
                    android:id="@+id/nineView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine2"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/three"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/two">

                <ImageView
                    android:id="@+id/nineView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine3"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/one">

                <ImageView
                    android:id="@+id/nineView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine4"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/five"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/four"
                android:layout_below="@+id/two">

                <ImageView
                    android:id="@+id/nineView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine5"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/six"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/five"
                android:layout_below="@+id/three">

                <ImageView
                    android:id="@+id/nineView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine6"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/seven"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_below="@id/four">

                <ImageView
                    android:id="@+id/nineView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine7"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/eight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/seven"
                android:layout_below="@id/five">

                <ImageView
                    android:id="@+id/nineView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine8"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/transparent"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/nine"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@id/eight"
                android:layout_below="@id/six">

                <ImageView
                    android:id="@+id/nineView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/no_image_nine" />

                <Button
                    android:id="@+id/nine9"
                    android:layout_width="105dp"
                    android:layout_height="130dp"
                    android:background="@color/transparent"/>
            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

</LinearLayout>